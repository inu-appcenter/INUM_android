<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.UploadCategoryFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_upload_category"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginTop="40dp"
            android:text="카테고리"
            android:textColor="@color/gunmetal"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ListView
            android:id="@+id/listview_upload_category"
            android:layout_width="210dp"
            android:layout_height="288dp"
            android:layout_marginHorizontal="75dp"
            android:layout_marginTop="44dp"
            android:divider="@null"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_upload_category"
            tools:listitem="@layout/item_listview_upload_category">

        </ListView>

    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>