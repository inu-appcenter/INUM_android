<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="301.5dp"
    android:layout_height="227.7dp"
    android:background="@drawable/popup_product_background"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/txt_sidi"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginTop="76.5dp"
        android:layout_marginBottom="103.2dp"
        android:gravity="center_horizontal"
        android:textColor="@color/gunmetal"
        android:lines="2"
        android:text="회원가입이 완료되었습니다.\n이메일 인증완료 후 로그인해주세요"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view10"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginBottom="57dp"
        android:background="@color/white_two"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>


    <Button
        android:id="@+id/btn_signupdi_submit"
        android:layout_width="168dp"
        android:layout_height="56dp"
        android:layout_marginStart="8dp"
        android:background="#00ffffff"
        android:textColor="#4a4a4a"
        android:text="확인"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view10" />


</androidx.constraintlayout.widget.ConstraintLayout>